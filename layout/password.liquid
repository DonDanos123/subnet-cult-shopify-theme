    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    :root {
      --terminal-green: #00ff41;
      --terminal-bg: #0a0a0a;
    }

    body {
      font-family: 'Courier New', monospace;
      background: var(--terminal-bg);
      color: var(--terminal-green);
      min-height: 100vh;
      display: flex;
      align-items: center;
      justify-content: center;
      padding: 2rem;
    }

    body::before {
      content: '';
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: repeating-linear-gradient(
        0deg,
        rgba(0, 0, 0, 0.15),
        rgba(0, 0, 0, 0.15) 1px,
        transparent 1px,
        transparent 2px
      );
      pointer-events: none;
      z-index: 1000;
    }

    .password-container {
      max-width: 600px;
      width: 100%;
      border: 2px solid var(--terminal-green);
      padding: 3rem;
      background: rgba(0, 0, 0, 0.9);
      text-align: center;
      position: relative;
      z-index: 1;
    }

    .password-logo {
      font-size: 2.5rem;
      margin-bottom: 2rem;
      text-shadow: 0 0 20px var(--terminal-green);
      letter-spacing: 4px;
    }

    .password-message {
      font-size: 1.2rem;
      margin-bottom: 2rem;
      opacity: 0.8;
    }

    .password-form {
      margin-bottom: 2rem;
    }

    .password-input {
      width: 100%;
      padding: 1rem;
      background: var(--terminal-bg);
      border: 1px solid var(--terminal-green);
      color: var(--terminal-green);
      font-family: 'Courier New', monospace;
      font-size: 1rem;
      margin-bottom: 1rem;
    }

    .password-button {
      width: 100%;
      padding: 1rem;
      background: transparent;
      border: 2px solid var(--terminal-green);
      color: var(--terminal-green);
      font-family: 'Courier New', monospace;
      font-size: 1rem;
      cursor: pointer;
      transition: all 0.3s;
    }

    .password-button:hover {
      background: var(--terminal-green);
      color: var(--terminal-bg);
      box-shadow: 0 0 20px var(--terminal-green);
    }

    @keyframes blink {
      0%, 50% { opacity: 1; }
      51%, 100% { opacity: 0; }
    }

    .blink {
      animation: blink 1s infinite;
    }
  


  
    
